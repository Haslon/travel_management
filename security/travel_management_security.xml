<odoo>
    <data noupdate="1"> <record id="module_category_travel_management" model="ir.module.category">
            <field name="name">Travel Management</field>
            <field name="description">User access levels for Travel Management module</field>
            <field name="sequence">25</field>
        </record>

        <record id="group_travel_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_travel_management"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="comment">Standard user, can view travel data and submit ratings.</field>
        </record>

        <record id="group_travel_agency" model="res.groups">
            <field name="name">Agency</field>
            <field name="category_id" ref="module_category_travel_management"/>
            <field name="implied_ids" eval="[(4, ref('group_travel_user'))]"/>
            <field name="comment">Agency user, can manage their agency's data, flights, and packages.</field>
        </record>

        <record id="group_travel_manager" model="res.groups">
            <field name="name">Manager / Administrator</field>
            <field name="category_id" ref="module_category_travel_management"/>
            <field name="implied_ids" eval="[(4, ref('group_travel_agency'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/> <field name="comment">Administrator, can manage all travel data, settings, and users.</field>
        </record>
    </data>
</odoo>